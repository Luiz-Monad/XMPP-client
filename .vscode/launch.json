{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Launch src/server.js",
      "type": "node",
      "program": "src/server.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": ".",
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": false,
      "request": "launch",
    },
    {
      "name": "gulp start",
      "type": "node",
      "program": "./node_modules/gulp/bin/gulp.js",
      "stopOnEntry": false,
      "args": [
        "start"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": false,
      "request": "launch",
    },
    {
      "name": "gulp client",
      "type": "node",
      "program": "./node_modules/gulp/bin/gulp.js",
      "stopOnEntry": false,
      "args": [
        "client"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "",
      "runtimeArgs": [
        "--inspect-brk",
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": false,
      "request": "launch",
    },
    {
      "name": "Attach",
      "type": "node",
      "sourceMaps": false,
      "request": "launch",
    }
  ]
}
