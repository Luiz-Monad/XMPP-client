      "muc": "chat.example.com",
      "startup": "http://localhost:8000",
      "admin": "admin",
      "gravatar": false,
      "keepalive": {
          "interval": 45,
          "timeout": 15
      },
      "cmd": [
        "docker", "run",
        "--name", "ejabberd",
        "-d",
        "-p", "5222:5222",
        "-p", "5280:5280",
        "-v", "./ejabberd.yml:/home/ejabberd/conf/ejabberd.yml",
        "ejabberd/ecs"
      ]
  }
