{
  "isDev": true,
  "http": {
      "baseUrl": "http://localhost:8000",
      "port": 8000,
      "key": "./fakekeys/privatekey.pem",
      "cert": "./fakekeys/certificate.pem"
  },
  "session": {
      "secret": "shhhhhh don't tell anyone ok?"
  },
  "server": {
